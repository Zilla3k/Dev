# Arquivo .editorconfig

# Definição da codificação de caracteres
[*]
charset = utf-8

# Definição do estilo de fim de linha
[*]
end_of_line = lf

# Definição do estilo de indentação
[*]
indent_style = space
indent_size = 4

# Definição do estilo de quebra de linha
[*.js]
max_line_length = 120
trim_trailing_whitespace = true
